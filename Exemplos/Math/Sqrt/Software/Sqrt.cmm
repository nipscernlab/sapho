#PRNAME Sqrt
#NUBITS 32
#NDSTAC 5
#SDEPTH 5
#NUIOIN 1
#NUIOOU 1
#NBMANT 23
#NBEXPO 8
#NUGAIN 128

float my_sqrt(float num)
{
    if (num == 0.0) return 0.0;
    
    int v = (((num << 1) >>> 24) + 22) >>> 1;         // pega o expoente
        v = ((((v-22) << 23) + (1 << 22)) << 1) >> 1; // monta o float

    float x; copy(v,x);

    x = 0.5 * (x+num/x);
    x = 0.5 * (x+num/x);
    x = 0.5 * (x+num/x);
    x = 0.5 * (x+num/x);

    return x;
}

void main() 
{
    float x[1000] "sqrt_x.txt";
    float a[1000] "sqrt_y.txt";
    float y,t,e;

    int j=0;
    while (j < 1000)
    {
        y = sqrt(x[j]);
        t = a[j];
        e = t - y;
        j++;
    }

    j=0;
    while (j < 1000)
    {
        y = my_sqrt(x[j]);
        t = a[j];
        e = t - y;
        j++;
    }
}