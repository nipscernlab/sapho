#PRNAME Seno
#NUBITS 32
#NDSTAC 5
#SDEPTH 5
#NUIOIN 1
#NUIOOU 1
#NBMANT 23
#NBEXPO 8
#NUGAIN 128

float seno_LUT(float x)
{
    float Seno_LUT[151] "Seno_LUT.txt";
    
    while (abs(x) > 3.141592653589793) x = x - sign(x, 6.283185307);

    float idxf = abs(x * 47.746482927568);  // 150.0/pi;
    int idx = idxf;

    if (idx == 150)
        return 0.0;
    else
    {
        float v = Seno_LUT[idx];
        return sign(x, v + (Seno_LUT[idx+1] - v) * (idxf - idx));
    }
}

void main() 
{
    float x[1000] "sin_x.txt";
    float a[1000] "sin_y.txt";
    float y,t,e;

    int j=0;
    while (j < 1000)
    {
        y = sin(x[j]);
        t = a[j];
        e = t - y;
        j++;
    }

    j=0;
    while (j < 1000)
    {
        y = seno_LUT(x[j]);
        t = a[j];
        e = t - y;
        j++;
    }
}